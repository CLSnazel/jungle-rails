<h1>Login</h1>

<%= form_for '/login' do |form|%>

  <div class="form-group">
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>
  <div class="form-group">
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>

  <%= form.submit "Submit", class: 'btn btn-primary' %>

<% end %>